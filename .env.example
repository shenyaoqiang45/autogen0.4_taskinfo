# LLM API Keys
# 至少需要配置一个 LLM 提供商
OPENAI_API_KEY=your_openai_api_key_here
# ANTHROPIC_API_KEY=your_anthropic_api_key_here  # 可选

# WhatAI 中转 API 配置（支持 Claude 和 GPT 系列）
# 用于 Planner (Claude 4.5) 和 Executor (GPT-5.2)
# WHATAI_API_KEY=your_whatai_api_key_here
# WHATAI_BASE_URL=https://api.whatai.cc/v1

# DeepSeek 官方 API 配置
# 用于 Red Team 和 Auditor (DeepSeek-R1 推理模型)
# DEEPSEEK_API_KEY=your_deepseek_api_key_here
# DEEPSEEK_BASE_URL=https://api.deepseek.com/v1

# MCP 工具 API Keys (可选但推荐)
# Tavily Search API - 用于网络搜索和引用查找
TAVILY_API_KEY=your_tavily_api_key_here

# 高德地图 API - 用于地理位置信息 (可选)
# AMAP_API_KEY=your_amap_api_key_here

# 模型配置
DEFAULT_MODEL=gpt-4-turbo-preview
TEMPERATURE=0.7
MAX_TOKENS=4096

# 日志级别
LOG_LEVEL=INFO
